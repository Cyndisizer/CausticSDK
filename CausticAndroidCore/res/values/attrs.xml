<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- see PadMatrix for impl -->
    <declare-styleable name="pad_matrix">
        <attr name="num_columns" format="integer" />
        <attr name="num_rows" format="integer" />
    </declare-styleable>
    <!-- see Button for impl -->
    <declare-styleable name="Button">
        <attr name="state_normal" format="boolean" />
        <attr name="state_checked" format="boolean" />
        <attr name="state_indeterminate" format="boolean" />
    </declare-styleable>
    <!-- see CompositeButton for impl -->
    <declare-styleable name="CompositeButton">
        <attr name="layout" format="reference" />
    </declare-styleable>
    <!-- see RangeView for impl -->
    <declare-styleable name="RangeView">
        <attr name="minimum" format="float" />
        <attr name="maximum" format="float" />
        <attr name="snap_interval" format="float" />
        <attr name="selected_value" format="float" />
    </declare-styleable>
    <!-- see Slider for impl -->
    <declare-styleable name="Slider">
        <attr name="text" format="string" />
        <attr name="thumb" format="reference" />
        <attr name="track" format="reference" />
        <attr name="track_background" format="reference" />
        <attr name="text_color" format="color" />
    </declare-styleable>

</resources>