<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox prefHeight="534.0" prefWidth="250.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.teotigraphix.libraryeditor.view.LibraryItemPaneController">
  <children>
    <HBox fx:id="toggleBar" prefHeight="-1.0" prefWidth="200.0" scaleY="1.0" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <ToggleButton id="" fx:id="sceneButton" mnemonicParsing="false" onAction="#buttonClickHandler" prefWidth="-1.0" selected="false" text="Scene" HBox.hgrow="ALWAYS">
          <toggleGroup>
            <ToggleGroup fx:id="libraryItemGroup" />
          </toggleGroup>
        </ToggleButton>
        <ToggleButton fx:id="patchButton" mnemonicParsing="false" onAction="#buttonClickHandler" prefWidth="-1.0" text="Patch" toggleGroup="$libraryItemGroup" HBox.hgrow="ALWAYS" />
        <ToggleButton fx:id="phraseButton" mnemonicParsing="false" onAction="#buttonClickHandler" text="Phrase " toggleGroup="$libraryItemGroup" HBox.hgrow="ALWAYS" />
      </children>
      <padding>
        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
      </padding>
      <rotationAxis>
        <Point3D z="1.0" />
      </rotationAxis>
    </HBox>
    <HBox prefHeight="-1.0" prefWidth="200.0">
      <children>
        <Label fx:id="titleLabel" alignment="CENTER_RIGHT" text="Label" />
      </children>
    </HBox>
    <StackPane fx:id="viewStack" prefHeight="150.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
        <ListView id="itemList" fx:id="sceneList" editable="false" prefHeight="-1.0" prefWidth="250.0" />
        <ListView fx:id="patchList" prefHeight="200.0" prefWidth="200.0" />
        <ListView fx:id="phraseList" prefHeight="200.0" prefWidth="200.0" />
      </children>
    </StackPane>
  </children>
</VBox>
