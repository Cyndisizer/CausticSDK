<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>

<fx:root type="HBox" spacing="5.0" fx:controller="com.teotigraphix.libraryeditor.view.MainToolBarController" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <Button fx:id="newButton" id="newButton" mnemonicParsing="false" onAction="#onNewButtonHandler" prefWidth="75.0" text="New" />
    <Button fx:id="loadButton" id="newButton" mnemonicParsing="false" onAction="#onLoadButtonHandler" prefWidth="75.0" text="Load" />
    <Button fx:id="saveButton" id="newButton" mnemonicParsing="false" onAction="#onSaveButtonHandler" prefWidth="75.0" text="Save" />
    <Button fx:id="importButton" id="newButton" mnemonicParsing="false" onAction="#onImportButtonHandler" prefWidth="75.0" text="Import" />
    <Button fx:id="previewButton" id="newButton" mnemonicParsing="false" onAction="#onPreviewButtonHandler" prefWidth="75.0" text="Preview" />
  </children>
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
  </padding>
</fx:root>
